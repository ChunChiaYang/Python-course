   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "#Set x and y\n",
    "x=np.linspace(-3,3,15)\n",
    "y=2**(-x)+2\n",
    "\n",
    "#Plot scatter\n",
    "plt.scatter(x,y)\n",
    "plt.title('y=2^(-x)+2')\n",
    "plt.ylabel('y',rotation=0)\n",
    "plt.xlabel('x')\n",
    "plt.grid()\n",
    "plt.show()"
   ]
  }
